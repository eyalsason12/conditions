print("hello user")
number = input("please insert number: ")
if int(number) <= 99:
    print("the number have got 2 digits")
elif int(number) <= 999:
    print("the number have got 3 digits")
elif int(number) <= 9999:
    print("the number have got 4 digits")
else:
    print("the number is <= 9 || > 9999")
